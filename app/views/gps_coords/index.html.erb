<h1>Listing gps_coords</h1>

<table>
  <thead>
    <tr>
      <th>When</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Altitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gps_coords.each do |gps_coord| %>
      <tr>
        <td><%= gps_coord.when %></td>
        <td><%= gps_coord.longitude %></td>
        <td><%= gps_coord.latitude %></td>
        <td><%= gps_coord.altitude %></td>
        <td><%= link_to 'Show', gps_coord %></td>
        <td><%= link_to 'Edit', edit_gps_coord_path(gps_coord) %></td>
        <td><%= link_to 'Destroy', gps_coord, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gps coord', new_gps_coord_path %>
